<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  


<bean id="veh" class="settereg.Vehicle">
<property name="no" value="2"></property>
<property name="name" value="car"></property>
</bean>

<bean id="emp" class="constructoreg.Emp">  
<constructor-arg value="105 type="int"></constructor-arg>  
<constructor-arg value="niha"> </constructor-arg> 
<constructor-arg value="15000" type="float"></constructor-arg> 
</bean> 


 
</beans>
  
